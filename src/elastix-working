#!/bin/bash
export DYLD_LIBRARY_PATH=/Applications/elastix_macosx64_v4.8/lib:$DYLD_LIBRARY_PATH
elastix -f ../data/./copd2/copd2_iBHCT.nii.gz -m ../data/./copd2/copd2_eBHCT.nii.gz -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd2/ -p ../parameter_files/Parameters.Par0011.affine-working.txt -p ../parameter_files/Parameters.Par0011.bspline1-working.txt
transformix -def ../data/copd2/copd2_300_iBH_xyz_r1_elastix.txt -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd2/ -tp ../registration-results/Par0011-pyr6-ss5000-it2000/copd2/TransformParameters.1.txt
elastix -f ../data/./copd4/copd4_iBHCT.nii.gz -m ../data/./copd4/copd4_eBHCT.nii.gz -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd4/ -p ../parameter_files/Parameters.Par0011.affine-working.txt -p ../parameter_files/Parameters.Par0011.bspline1-working.txt
transformix -def ../data/copd4/copd4_300_iBH_xyz_r1_elastix.txt -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd4/ -tp ../registration-results/Par0011-pyr6-ss5000-it2000/copd4/TransformParameters.1.txt
elastix -f ../data/./copd3/copd3_iBHCT.nii.gz -m ../data/./copd3/copd3_eBHCT.nii.gz -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd3/ -p ../parameter_files/Parameters.Par0011.affine-working.txt -p ../parameter_files/Parameters.Par0011.bspline1-working.txt
transformix -def ../data/copd3/copd3_300_iBH_xyz_r1_elastix.txt -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd3/ -tp ../registration-results/Par0011-pyr6-ss5000-it2000/copd3/TransformParameters.1.txt
elastix -f ../data/./copd1/copd1_iBHCT.nii.gz -m ../data/./copd1/copd1_eBHCT.nii.gz -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd1/ -p ../parameter_files/Parameters.Par0011.affine-working.txt -p ../parameter_files/Parameters.Par0011.bspline1-working.txt
transformix -def ../data/copd1/copd1_300_iBH_xyz_r1_elastix.txt -out ../registration-results/Par0011-pyr6-ss5000-it2000/copd1/ -tp ../registration-results/Par0011-pyr6-ss5000-it2000/copd1/TransformParameters.1.txt
