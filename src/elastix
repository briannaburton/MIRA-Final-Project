#!/bin/bash
export DYLD_LIBRARY_PATH=/Applications/elastix_macosx64_v4.8/lib:$DYLD_LIBRARY_PATH

elastix -f ../data/./copd1/copd1_iBHCT.nii.gz -m ../data/./copd1/copd1_eBHCT.nii.gz -out ../registration-results/challenge-day-results/copd1/ -p ../parameter_files/FINAL-affine.txt -p ../parameter_files/FINAL-bsplines.txt
transformix -def ../data/copd1/copd1_300_iBH_xyz_r1_elastix.txt -out ../registration-results/challenge-day-results/copd1/ -tp ../registration-results/challenge-day-results/copd1/TransformParameters.1.txt
elastix -f ../data/./copd2/copd2_iBHCT.nii.gz -m ../data/./copd2/copd2_eBHCT.nii.gz -out ../registration-results/challenge-day-results/copd2/ -p ../parameter_files/FINAL-affine.txt -p ../parameter_files/FINAL-bsplines.txt
transformix -def ../data/copd2/copd2_300_iBH_xyz_r1_elastix.txt -out ../registration-results/challenge-day-results/copd2/ -tp ../registration-results/challenge-day-results/copd2/TransformParameters.1.txt













